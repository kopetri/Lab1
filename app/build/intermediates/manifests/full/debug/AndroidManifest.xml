<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sebastianhartwig.com.lab1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />

    <uses-feature
        android:name="android.hardware.sensor.light"
        android:required="true" />
    <uses-feature
        android:name="android.hardware.sensor.accelerometer"
        android:required="true" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="sebastianhartwig.com.muc_15_01_14.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="sebastianhartwig.com.muc_15_01_14.LightActivity"
            android:label="@string/title_activity_light" >
        </activity>
        <activity
            android:name="sebastianhartwig.com.muc_15_01_14.OrientationActivity"
            android:label="@string/title_activity_orientation" >
        </activity>
        <activity
            android:name="sebastianhartwig.com.muc_15_01_14.GPSActivity"
            android:label="@string/title_activity_gps" >
        </activity>
        <activity
            android:name="sebastianhartwig.com.muc_15_01_14.AccelerometerActivity"
            android:label="@string/title_activity_accelerometer" >
        </activity>
    </application>

</manifest>